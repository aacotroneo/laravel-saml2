{
    "name": "nirajp/laravel-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) for multiple IDPs. Forked from aacotroneo/laravel-saml2.",
    "keywords": ["laravel","saml", "saml2", "onelogin"],
    "homepage": "https://github.com/nirajp/laravel-saml2",
    "license": "MIT",
    "authors": [
        {
            "name": "aacotroneo",
            "email": "aacotroneo@gmail.com"
        },
        {
            "name": "Niraj Patkar",
            "email": "niraj@alphonso.in"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0.0",
        "onelogin/php-saml": "2.9.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Aacotroneo\\Saml2\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
